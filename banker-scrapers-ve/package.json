{
  "name": "banker-scrapers-ve",
  "version": "1.0.0",
  "description": "TypeScript library for scraping Venezuelan bank accounts (Banesco & BNC) using Playwright. Session persistence, smart timeouts, and optimized performance.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./banesco": {
      "types": "./dist/banks/banesco/index.d.ts",
      "import": "./dist/banks/banesco/index.js"
    },
    "./bnc": {
      "types": "./dist/banks/bnc/index.d.ts",
      "import": "./dist/banks/bnc/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build",
    "example:banesco": "tsx src/banks/banesco/examples/basic-usage.ts",
    "example:banesco-hybrid": "tsx src/banks/banesco/examples/hybrid-usage.ts",
    "example:bnc": "tsx src/banks/bnc/examples/http-usage.ts",
    "postinstall": "npx playwright install chromium"
  },
  "keywords": [
    "venezuela",
    "banking",
    "banesco",
    "bnc",
    "scraping",
    "automation",
    "playwright",
    "typescript",
    "web-scraping",
    "bank-scraper",
    "financial-data",
    "banking-automation",
    "fintech",
    "venezuela-banks",
    "transaction-scraping",
    "browser-automation",
    "headless-browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danicanod/banker-scrapers-ve.git"
  },
  "bugs": {
    "url": "https://github.com/danicanod/banker-scrapers-ve/issues"
  },
  "homepage": "https://github.com/danicanod/banker-scrapers-ve#readme",
  "author": {
    "name": "Daniel Sanchez",
    "email": "danicanod@gmail.com",
    "url": "https://github.com/danicanod"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/danicanod"
  },
  "files": [
    "dist/**/*",
    "*.md",
    "env.example"
  ],
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.10.2",
    "@types/tough-cookie": "^4.0.5",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "dotenv": "^16.5.0",
    "playwright": "^1.49.1",
    "playwright-core": "^1.57.0",
    "tough-cookie": "^6.0.0"
  }
}
