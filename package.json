{
  "name": "@danicanod/banker-venezuela",
  "version": "2.1.0",
  "description": "TypeScript library for connecting to Venezuelan bank accounts (Banesco & BNC). Hybrid Playwright/HTTP architecture for reliability and speed.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./banesco": {
      "types": "./dist/banks/banesco/index.d.ts",
      "import": "./dist/banks/banesco/index.js"
    },
    "./bnc": {
      "types": "./dist/banks/bnc/index.d.ts",
      "import": "./dist/banks/bnc/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/ convex/ scripts/ --ignore-pattern 'convex/_generated/**'",
    "lint:fix": "eslint src/ convex/ scripts/ --ignore-pattern 'convex/_generated/**' --fix",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build",
    "example:banesco": "tsx src/banks/banesco/examples/basic-usage.ts",
    "example:banesco-hybrid": "tsx src/banks/banesco/examples/hybrid-usage.ts",
    "example:bnc": "tsx src/banks/bnc/examples/http-usage.ts",
    "example:bnc-http": "tsx src/banks/bnc/examples/http-usage.ts",
    "example:performance": "tsx src/shared/examples/performance-optimization.ts",
    "capture:banesco": "CAPTURE_BANK=banesco tsx src/shared/examples/capture-network-flow.ts",
    "capture:bnc": "CAPTURE_BANK=bnc tsx src/shared/examples/capture-network-flow.ts",
    "test": "tsx scripts/_sync-utils.test.ts",
    "sync": "tsx scripts/banesco-sync.ts",
    "sync:banesco": "tsx scripts/banesco-sync.ts",
    "sync:bnc": "tsx scripts/bnc-sync.ts",
    "postinstall": "npx playwright install chromium"
  },
  "keywords": [
    "venezuela",
    "banking",
    "banesco",
    "bnc",
    "scraping",
    "automation",
    "playwright",
    "typescript",
    "web-scraping",
    "bank-scraper",
    "financial-data",
    "banking-automation",
    "fintech",
    "venezuela-banks",
    "transaction-scraping",
    "browser-automation",
    "headless-browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danicanod/banker-venezuela.git"
  },
  "bugs": {
    "url": "https://github.com/danicanod/banker-venezuela/issues"
  },
  "homepage": "https://github.com/danicanod/banker-venezuela#readme",
  "author": {
    "name": "Daniel Sanchez",
    "email": "danicanod@gmail.com",
    "url": "https://github.com/danicanod"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/danicanod"
  },
  "files": [
    "dist/**/*",
    "*.md",
    "env.example"
  ],
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.10.2",
    "@types/tough-cookie": "^4.0.5",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.6.0",
    "@notionhq/client": "^2.3.0",
    "cheerio": "^1.1.2",
    "convex": "^1.31.5",
    "dotenv": "^16.5.0",
    "playwright": "^1.49.1",
    "playwright-core": "^1.57.0",
    "tough-cookie": "^6.0.0"
  }
}
